<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="187.0" prefWidth="290.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="Controllers.Dialogs.EditStockDialog">
    <children>
        <Label fx:id="title" alignment="CENTER" layoutX="54.0" layoutY="25.0" prefHeight="27.0" prefWidth="182.0"
               text="Edit product details">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label layoutX="19.0" layoutY="77.0" prefHeight="17.0" prefWidth="62.0" text="Type:"/>
        <Label layoutX="19.0" layoutY="111.0" prefHeight="17.0" prefWidth="62.0" text="Quantity:"/>
        <TextField fx:id="type" layoutX="87.0" layoutY="73.0"/>
        <TextField fx:id="quantity" layoutX="87.0" layoutY="107.0"/>
        <Button layoutX="162.0" layoutY="148.0" mnemonicParsing="false" onAction="#saveButtonClicked" text="Save"/>
        <Button layoutX="210.0" layoutY="148.0" mnemonicParsing="false" onAction="#cancelButtonClicked" text="Cancel"/>
    </children>
</AnchorPane>

