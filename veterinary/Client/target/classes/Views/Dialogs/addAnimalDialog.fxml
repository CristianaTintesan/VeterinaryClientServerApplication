<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="219.0" prefWidth="386.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="Controllers.Dialogs.AddAnimalDialog">
    <children>
        <Button layoutX="225.0" layoutY="170.0" mnemonicParsing="false" onAction="#cancelButtonClicked"
                prefHeight="27.0" prefWidth="60.0" text="Cancel"/>
        <Button layoutX="309.0" layoutY="170.0" mnemonicParsing="false" onAction="#saveButtonClicked" prefHeight="27.0"
                prefWidth="60.0" text="Save"/>
        <Label alignment="CENTER" layoutX="131.0" layoutY="25.0" prefHeight="27.0" prefWidth="124.0" text="Add animal">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label layoutX="43.0" layoutY="71.0" prefHeight="25.0" prefWidth="69.0" text="Name: "/>
        <Label layoutX="43.0" layoutY="115.0" prefHeight="25.0" prefWidth="69.0" text="Owner ID: "/>
        <TextField fx:id="name" layoutX="160.0" layoutY="71.0"/>
        <TextField fx:id="owner" layoutX="160.0" layoutY="115.0"/>
    </children>
</AnchorPane>
