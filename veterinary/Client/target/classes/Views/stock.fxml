<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="Controllers.StockPage">
    <children>
        <Label layoutX="269.0" layoutY="14.0" text="Stock">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <TableView fx:id="tableView" layoutX="186.0" layoutY="44.0" prefHeight="356.0" prefWidth="600.0"
                   AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                   AnchorPane.topAnchor="44.0">
            <columns>
                <TableColumn fx:id="id" prefWidth="133.0" text="ID"/>
                <TableColumn fx:id="productType" prefWidth="306.0" text="Type"/>
                <TableColumn fx:id="productQuantity" prefWidth="160.0" text="Quantity"/>
            </columns>
        </TableView>
        <Button fx:id="addButton" layoutX="534.0" layoutY="15.0" mnemonicParsing="false" onAction="#addProduct"
                text="Add" visible="false"/>
    </children>
</AnchorPane>
